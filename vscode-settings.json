{
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "editor.guides.bracketPairs": "active",
    "explorer.confirmDelete": false,
    "python.defaultInterpreterPath": "/usr/local/bin/python3",
    "editor.detectIndentation": true,
    "[javascript]": {
      "editor.tabSize": 2,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[ruby]": {
      "editor.tabSize": 2
    },
    "[python]": {
      "editor.tabSize": 4,
      "editor.formatOnType": true
    },
    "[vue]": {
      "editor.tabSize": 2
    },
    "[html]": {
      "editor.tabSize": 2
    },
    "[yaml]": {
      "editor.tabSize": 2
    },
    "editor.accessibilitySupport": "off",
    "redhat.telemetry.enabled": false,
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.colorTheme": "Dainty â€“ Material Theme Palenight",
    "RainbowBrackets.depreciation-notice": false,
    "settingsSync.ignoredExtensions": ["jimzhang.jimthon"],
    "editor.lineNumbers": "relative",
    "editor.tokenColorCustomizations": {
      "textMateRules": [
        {
          "scope": "keyword.other.dotenv",
          "settings": {
            "foreground": "#FF000000"
          }
        }
      ]
    },
    "explorer.confirmDragAndDrop": false,
    "workbench.iconTheme": "material-icon-theme",
    "editor.inlineSuggest.enabled": true,
    "ruby.rubocop.configFilePath": "/Users/hydenpolikoff/development/motive-inc/motive/rubocop.yml",
    "ruby.rubocop.executePath": "/Users/hydenpolikoff/.rbenv/shims/rubocop",
    "editor.minimap.enabled": false,
    "github.copilot.editor.enableAutoCompletions": true,
    "go.toolsManagement.autoUpdate": true,
    "explorer.fileNesting.patterns": {
      "*.ts": "${capture}.js",
      "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
      "*.jsx": "${capture}.js",
      "*.tsx": "${capture}.ts",
      "tsconfig.json": "tsconfig.*.json",
      "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
      "*.sqlite": "${capture}.${extname}-*",
      "*.db": "${capture}.${extname}-*",
      "*.sqlite3": "${capture}.${extname}-*",
      "*.db3": "${capture}.${extname}-*",
      "*.sdb": "${capture}.${extname}-*",
      "*.s3db": "${capture}.${extname}-*"
    }
  }
  